<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
               
               <bean id="customer" class="com.vm.entity.Customer" autowire="byName">
               <property name="id" value="100"></property>
               <property name="name" value="jai"></property>
               
               </bean>
               
               <bean id="order" class="com.vm.entity.Order" >
               <constructor-arg value="123"></constructor-arg>
               <constructor-arg  value="laptop"></constructor-arg>
               <constructor-arg value="2"></constructor-arg>
               </bean>
               
               <bean id="order1" class="com.vm.entity.Order" >
               <constructor-arg value="456"></constructor-arg>
               <constructor-arg  value="mobile"></constructor-arg>
               <constructor-arg value="1"></constructor-arg>
               </bean>
               
                <bean id="address" class="com.vm.entity.Address" >
               <constructor-arg value="192"></constructor-arg>
               <constructor-arg  value="yenugonda"></constructor-arg>
               <constructor-arg value="mbnr"></constructor-arg>
               </bean>
               
               </beans>