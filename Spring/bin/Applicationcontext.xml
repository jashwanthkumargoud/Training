<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
               
               <bean id="emp" class="com.vm.entity.Employee">
               <property name="id" value="1"></property>
               <property name="name" value="john"></property>
               <property name="dept" value="dev"></property>
               <property name="address" ref="ad"></property>
               </bean>
               
               <bean id="p" class="com.vm.entity.Product">
               <constructor-arg value="1"></constructor-arg>
               <constructor-arg value="mobile"></constructor-arg>
               <constructor-arg value="cse"></constructor-arg>
               </bean>
               
               <bean id="ad" class="com.vm.entity.Address">
               <property name="doorno" value="1223"></property>
               <property name="streetName" value="yenugonda"></property>
               <property name="city" value="Mahabubnagar"></property>
               </bean>
               
               <bean id="d" class="com.vm.entity.Department">
               <property name="noofdept" value="12"></property>
              
               </bean>
               
               <bean id="clg" class="com.vm.entity.College">
               <property name="name" value="klu"></property>
               <property name="college_dept" value="klu"></property>
               <property name="city" value="Mahabubnagar"></property>
               </bean>
</beans>